
{
    "version": 2,
    "builds": [
      {
        "src": "cps/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/apps_static/(.*)",
        "dest": "/apps_static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "cps/wsgi.py"
      }
    ]
  
  }
  